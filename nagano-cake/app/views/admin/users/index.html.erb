<h2>会員一覧画面</h2>

<table>
	<thead>
		<tr>
			<th>会員ID</th>
			<th>氏名</th>
			<th>メールアドレス</th>
			<th>ステータス</th>
		</tr>
	</thead>
	<tbody>

		<% @users.each do |user| %>
		<tr>
			<td><%= user.id%></td>
			<td><%= user.name%></td>
			<td><%= user.email%></td>
			<td><%= %></td>
		</tr>
		<% end %>

	</tbody>
</table>